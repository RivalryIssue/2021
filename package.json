{
  "name": "sourdough",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://RivalryIssue.github.io",
  "devDependencies": {
    "archieml": "^0.4.2",
    "googleapis": "^52.0.0",
    "html-entities": "^1.3.1",
    "htmlparser2": "^4.1.0",
    "husky": "^4.3.0",
    "nunjucks": "^3.2.1",
    "parcel": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.4.3",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "sass": "^1.26.7"
  },
  "scripts": {
    "deploy": "gh-pages -d dist",
    "fetch-doc": "node scripts/gdoc.js",
    "render-html": "node scripts/render.js",
    "build": "yarn run parcel build --public-url ./ src/index.html",
    "dev": "yarn run parcel src/index.html"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "src/data",
        "staticOutDir": "data"
      }
    ],
    "watcherGlob": "src/data/*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.9.1",
    "@types/react": "^16.8.6",
    "bootstrap": "^4.5.3",
    "gh-pages": "^3.1.0",
    "prop-types": "^15.5.8",
    "pym.js": "^1.3.2",
    "react": "^16.8.0",
    "react-bootstrap": "^1.3.0",
    "react-collapsible": "^2.8.1",
    "react-countdown": "^2.3.0",
    "react-dom": "^16.8.0",
    "react-is": ">= 16.8.0",
    "react-player": "^2.6.2",
    "react-scroll": "^1.8.1",
    "styled-components": "^5.2.0"
  }
}
